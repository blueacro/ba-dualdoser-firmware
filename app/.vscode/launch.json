{
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "build/dualdoser-nobl.elf",
            "name": "Debug Microcontroller",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "serverpath": "/usr/bin/JLinkGDBServer",
            "armToolchainPath": "/usr/bin",
            "device": "ATSAMD11D14A",
            "interface": "swd",
            "serialNumber": "", // add J-Link serial number if having multiple attached the same time.
            "runToEntryPoint": "main",
            "svdFile": "${workspaceRoot}/ATSAMD11D14AU.svd",
            "preLaunchTask": "Build Project"
        }
    ]
}
